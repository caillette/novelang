gunit AntlrPart ;

@header {
  package novelang.parser.antlr ;
}

title :

"'some title" -> (TITLE (WORD some) (WORD title))
"'some title !" -> (TITLE (WORD some) (WORD title) (PUNCTUATION_SIGN SIGN_EXCLAMATIONMARK))

identifier :
"identifier" -> (IDENTIFIER (WORD identifier))



